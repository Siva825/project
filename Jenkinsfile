pipeline{
    agent any
    stages{
        stage('build'){
            tools{
                jdk 'jdk17'
            }
            steps{
                 script{
                    	def course='devops'
                    	if (course=='devops1')
                    	println "application is success"
                    	else
                    	println"application fail"
                }
                 
            }
        }
    }
}
